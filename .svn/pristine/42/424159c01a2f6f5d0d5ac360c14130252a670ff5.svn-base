/* CLEARFIX HACK */
.row,.clr,.group{ zoom: 1; } /* IE6 */
*:first-child+html .row,*:first-child+html .clr,*:first-child+html .group{ zoom: 1; } /* IE7 */
/* RESET
----------------------------------------------------------------------------------------------------*/
a,blockquote, body { font-family: inherit; font-size: 100%; font-weight: inherit; font-style: inherit; vertical-align: baseline; 
white-space: normal; text-align: left; margin: 0; padding: 0; border: 0; outline: 0;background: transparent;}
ol, ul, li { list-style: none; }
blockquote, q { quotes: none; }
:focus { outline: 0; }
table { border-collapse: collapse; border-spacing: 0; }
* {-webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box;}

